# Sample configuration file for Cynetics CLI
# Rename this to config.yaml to use

model_providers:
  openai:
    api_key: "sk-..."  # Replace with your OpenAI API key
    model: "gpt-4"
  ollama:
    host: "http://localhost:11434"
    model: "llama3"

tools:
  enabled:
    - "file_manager"
    - "web_search"

tui_enabled: true